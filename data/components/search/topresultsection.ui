<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <template class="TopResultSection" parent="GtkBox">
    <property name="valign">1</property>
    <property name="orientation">1</property>
    <child>
      <object class="GtkBox">
        <property name="margin-top">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="margin-bottom">6</property>
        <child>
          <object class="GtkLabel" id="title">
            <property name="label" translatable="true">Top Result</property>
            <style>
              <class name="title-2" />
            </style>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwBreakpointBin">
        <property name="width-request">128</property>
        <property name="height-request">128</property>
        <child>
          <object class="AdwBreakpoint">
            <condition>max-width: 800sp</condition>
            <setter object="box" property="orientation">vertical</setter>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="box">
            <property name="spacing">6</property>
            <child>
              <object class="GtkListBox" id="content">
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <property name="selection-mode">0</property>
                <style>
                  <class name="background" />
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>